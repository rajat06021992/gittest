		<!-- __mMerch_viewcount_html -->
		<div class="moment_count"></div>
		<!-- __mMerch_menu_open_html -->
		<div class="mpopup moment_popup bottom right menu">
			<i class='fa fa-bars'></i>
		</div>
		<!-- __mMerch_save_html -->
		<div class="mpopup moment_popup top right done"></div>
		<!-- __mMerch_publisher_html -->
		<div class="mpopup moment_popup top right menu">
			<img src='' alt='' width="100%" height="100%" onError="$($(this).closest('.mthumb')).parent().find('.mthumb-part').remove();$(this).closest('.mthumb').remove();" />
		</div>
		<!-- __mMerch_cancel_html -->
		<div class="mpopup moment_popup bottom right cancel"></div>
		<!-- __mMerch_timeadjust_left_html -->
		<div class="mpopup moment_popup top left go_left">
			<i class='fa fa-backward'></i>
		</div>
		<!-- __mMerch_timeadjust_right_html -->
		<div class="mpopup moment_popup bottom left go_right">
			<i class='fa fa-forward'></i>
		</div>
		<!-- __mMerch_delay_html -->
		<div class="moment_barrier hidden"></div>
		<!-- __mMerch_menu_open_clickregion_html -->
		<div class="ghost_menu bottom right"></div>
		<!-- __mMerch_menu_publisher_clickregion_html -->
		<div class="ghost_menu top right"></div>
		<!-- __mMerch_fill_html -->
		<div class="fill hidden"></div>
		<!-- __moment_arrow_html -->
		<div class="moment_anchor arrow-down"></div>
	</a>
</script>

