<!-- pPromo_carousel_item_html -->
<script type='text/template' id='pPromo_item_template'>
	<div class='pPromo_item clearfix' style="width:90%;margin:auto;">
      <!-- pPromo_item_thumb_html -->
		<div class='pPromo_thumb'>
			<img class='pPromo_thumb_1' width="90%" height="100%"/>
			<img class='pPromo_thumb_2' width="90%" height="90%"/>
			<img class='pPromo_thumb_3' width="90%" height="80%"/>
			<a href='#' class='pPromo_thumb_play vmiddle'><span class='fa fa-play-circle-o'></span></a>
		</div>
      <!-- pPromo_item_detail_html -->
		<div class='section_right'>
			<a class='pPromo_title'></a>
			<div class='clearfix'>
				<div style="width:23%;float:left;">
					<a href='#' class='pPromo_user_image'>
						<img class='user_image' style='border-radius:50%;' width="100%" height="100%"/>
					</a>
				</div>
				<div style="width:77%;float:left;">
					<div class='pPromo_detail'>
						<div class='pMerch_title tags'></div>
						<div class='pMerch_title'><span class='pPromo_detail_moment_count'></span><span class='min-tablet pPromo_detail_moment_count_suffix'></span> by <span class='pPromo_detail_publisher_name'></span></div>
						<div class='pMerch_title'>Remixed <span class='pPromo_detail_time_ago'></span> ago</div>
						<div class='pMerch_title pPromo_detail_view_count'></div>
					</div>
				</div>
			</div>
		</div>
	</div>
</script>

<!-- pPromo_carousel_html -->
<script type='text/template' id='pPromo_carousel_template'>
	<div id="pPromo_carousel" class="carousel slide" data-ride="carousel">
		<!-- Indicators -->
		<ol class="carousel-indicators">
		</ol>

		<!-- Wrapper for slides -->
		<div class="carousel-inner" role="listbox">
		</div>

		<!-- Left and right controls -->
		<a class="left pPromo_carousel_left carousel-control" href="#pPromo_carousel" role="button" data-slide="prev">
			<span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
			<span class="sr-only">Previous</span>
		</a>
		<a class="right pPromo_carousel_right carousel-control" href="#pPromo_carousel" role="button" data-slide="next">
			<span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
			<span class="sr-only">Next</span>
		</a>
	</div>
	<style>
	.carousel-inner > .item > img,
	.carousel-inner > .item > a > img {
		width: 70%;
		margin: auto;
	}
  </style>
</script>
