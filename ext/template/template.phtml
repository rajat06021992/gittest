	data-image="{{thumbnail_background_image}}" data-rows="{{thumbnail_background_image_rows}}" data-columns="{{thumbnail_background_image_columns}}" data-position="{{thumbnail_background_image_position}}" data-tag="{{tag}}" data-count_played="{{count_played}}" data-count_played_dup="{{count_played_dup}}"
	data-publisher="{{user_id}}">
		<!-- __mMerch_thumb_html -->
		<div class="mthumbnail moment_thumbnail">
			<img width='100%' height='100%' />
		</div>
		<!-- __mMerch_play_icon_html -->
